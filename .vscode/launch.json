    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Debug app.js",
            "program": "${workspaceFolder}/app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal"
        },
        

        
        {
            "type": "node",
            "request": "attach",
            "name": "load my ts",
            "restart":true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\test\\src\\index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/grx-web/**/*.js"
            ]
        }
    ]
}